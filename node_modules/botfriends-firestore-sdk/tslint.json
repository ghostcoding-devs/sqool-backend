{
  "extends": "tslint-config-airbnb",
  "rules": {
    "ter-indent": false,
    "whitespace": false,
    "quotemark": false,
    "max-line-length": [
      true,
      200
    ],
    "import-name": false,
    "no-increment-decrement": false,
    "align": false,
    "object-curly-spacing": false,
    "no-duplicate-imports": false,
    "trailing-comma": false,
    "ter-arrow-parens": false,
    "function-name": false
  }
}